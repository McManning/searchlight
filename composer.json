{
    "name": "mcmanning/searchlight",
    "description": "Searchkit GraphQL API support for Lighthouse",
    "type": "library",
    "authors": [
        {
            "name": "Chase McManning",
            "email": "mcmanning.1@osu.edu"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "ci": "vendor/bin/phpunit -d --without-creating-snapshots"
    },
    "autoload": {
        "psr-4": {
            "McManning\\Searchlight\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^8.83",
        "elasticsearch/elasticsearch": "^7"
    },
    "require-dev": {
        "nuwave/lighthouse": "^5.26",
        "phpunit/phpunit": "^8",
        "orchestra/testbench": "^6.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "McManning\\Searchlight\\ServiceProvider"
            ]
        }
    }
}
